{
  "name": "deep-research-agent",
  "version": "1.0.0",
  "description": "LangGraph-powered deep research agent with streaming interface",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "npm run dev",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "setup": "npm install && echo 'Run: npm start to begin'",
    "backend:install": "pip install -r backend/requirements.txt",
    "backend:test": "cd backend && python -m main test",
    "backend:config": "cd backend && python -m main config",
    "backend:start": "cd backend && python -m uvicorn api:app --reload --host 0.0.0.0 --port 8000",
    "backend:only": "npm run backend:start",
    "frontend:only": "npm run dev",
    "test": "npm run backend:test",
    "langgraph:dev": "./venv/bin/langgraph dev --config langgraph.json --port 4242",
    "deploy:build": "./deploy.sh build",
    "deploy:start": "./deploy.sh start", 
    "deploy:stop": "./deploy.sh stop",
    "deploy:restart": "./deploy.sh restart",
    "deploy:logs": "./deploy.sh logs",
    "deploy:clean": "./deploy.sh clean"
  },
  "keywords": [
    "langgraph",
    "ai",
    "research",
    "agent",
    "react",
    "typescript"
  ],
  "dependencies": {
    "@langchain/core": "^0.3.61",
    "@langchain/langgraph-sdk": "^0.0.89",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-collapsible": "1.1.3",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "2.1.6",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "1.1.2",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "class-variance-authority": "0.7.1",
    "clsx": "^2.1.1",
    "langchain": "^0.3.29",
    "lucide-react": "^0.523.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "2.1.7",
    "recharts": "^2.15.4",
    "tailwind-merge": "^3.3.1",
    "vaul": "^0.9.9"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.0.0-alpha.31",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "tailwindcss": "^4.0.0-alpha.31",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
